{
  "name": "argon-design-system-pro-react",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.15.1",
    "@glidejs/glide": "3.4.1",
    "@laylazi/bootstrap-rtl": "^4.5.3-1",
    "@material-ui/core": "^4.11.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "@tinymce/tinymce-react": "^3.9.0",
    "axios": "^0.21.1",
    "bootstrap-v4-rtl": "^4.5.2-1",
    "choices.js": "9.0.1",
    "env-cmd": "^10.1.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "headroom.js": "0.11.0",
    "jquery": "^3.5.1",
    "moment": "2.27.0",
    "node-sass": "4.14.1",
    "nouislider": "14.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-datetime": "2.16.3",
    "react-dom": "16.13.1",
    "react-excel-renderer": "^1.1.0",
    "react-facebook-login": "^4.1.1",
    "react-github-button": "0.1.11",
    "react-google-login": "^5.1.20",
    "react-google-maps": "9.4.5",
    "react-paypal-button-v2": "^2.6.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-signature-canvas": "^1.0.3",
    "reactstrap": "8.5.1"
  },
  "scripts": {
    "start:local": "env-cmd -f .env.local react-scripts start",
    "build:dev": "rm -rf node_modules/ && rm -rf package-lock.json && env-cmd -f .env.development react-scripts build && gulp licenses",
    "build:prod": "rm -rf node_modules/ && rm -rf package-lock.json && env-cmd -f .env.production react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-design-system.scss src/assets/css/argon-design-system.css",
    "minify-sass": "node-sass src/assets/scss/argon-design-system.scss src/assets/css/argon-design-system.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-design-system.scss src/assets/css/argon-design-system.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "3.9.7",
    "@types/googlemaps": "3.39.11",
    "@types/markerclustererplus": "2.1.33"
  }
}
